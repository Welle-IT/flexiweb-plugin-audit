{
  "name": "@flexiweb/audit",
  "version": "3.65.2",
  "description": "Audit plugin for Flexiweb powered by PayloadCMS. Adds audit fields to collections, provides the possibility to full-Audit using dedicated collection.",
  "license": "SEE LICENSE IN LICENSE.md",
  "type": "module",
  "repository": "https://github.com/Welle-IT/flexiweb-plugin-audit",
  "author": "flexiweb",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./types": {
      "import": "./dist/types.js",
      "types": "./dist/types.d.ts",
      "default": "./dist/types.js"
    },
    "./utils": {
      "import": "./dist/utils.js",
      "types": "./dist/utils.d.ts",
      "default": "./dist/utils.js"
    },
    "./constants": {
      "import": "./dist/constants.js",
      "types": "./dist/constants.d.ts",
      "default": "./dist/constants.js"
    }
  },
  "keywords": [
    "payloadcms",
    "plugin",
    "flexiweb",
    "audit"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm copyfiles && pnpm build:types && pnpm build:swc",
    "build:swc": "swc ./src -d ./dist --config-file .swcrc --strip-leading-paths",
    "build:types": "tsc --outDir dist --rootDir ./src",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png,json}\" dist/",
    "dev": "next dev dev --turbo",
    "dev:generate-importmap": "pnpm dev:payload generate:importmap",
    "dev:generate-types": "pnpm dev:payload generate:types",
    "dev:payload": "cross-env PAYLOAD_CONFIG_PATH=./dev/payload.config.ts payload",
    "generate:importmap": "pnpm dev:generate-importmap",
    "generate:types": "pnpm dev:generate-types",
    "lint": "eslint",
    "lint:fix": "eslint ./src --fix",
    "prepublishOnly": "pnpm clean && pnpm build",
    "test": "pnpm test:int && pnpm test:e2e",
    "test:e2e": "playwright test",
    "test:int": "vitest"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@flexiweb/core": "3.66.0",
    "@payloadcms/db-sqlite": "3.66.0",
    "@payloadcms/eslint-config": "3.9.0",
    "@payloadcms/next": "3.66.0",
    "@payloadcms/richtext-lexical": "3.66.0",
    "@payloadcms/translations": "3.66.0",
    "@payloadcms/ui": "3.66.0",
    "@playwright/test": "^1.52.0",
    "@swc-node/register": "1.10.9",
    "@swc/cli": "0.6.0",
    "@types/node": "^22.5.4",
    "@types/react": "19.2.1",
    "@types/react-dom": "19.2.1",
    "copyfiles": "2.4.1",
    "cross-env": "^10.0.0",
    "eslint": "^9.23.0",
    "eslint-config-next": "15.4.7",
    "graphql": "^16.8.1",
    "next": "15.4.8",
    "open": "^10.1.0",
    "payload": "3.66.0",
    "prettier": "^3.4.2",
    "qs-esm": "7.0.2",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "rimraf": "6.0.1",
    "sharp": "0.34.4",
    "sort-package-json": "^3.4.0",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.2"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "./types": {
        "import": "./dist/types.js",
        "types": "./dist/types.d.ts",
        "default": "./dist/types.js"
      },
      "./utils": {
        "import": "./dist/utils.js",
        "types": "./dist/utils.d.ts",
        "default": "./dist/utils.js"
      },
      "./constants": {
        "import": "./dist/constants.js",
        "types": "./dist/constants.d.ts",
        "default": "./dist/constants.js"
      }
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  },
  "registry": "https://registry.npmjs.org/",
  "peerDependencies": {
    "@flexiweb/core": "3.66.0"
  }
}
